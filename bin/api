#!rails runner
rows = ApiList.collect do |elem|
  puts elem.ret_body_command

  {}.tap do |h|
    h[:url] = elem.url.sub("http://localhost:3000/", "")
    h[:format] = elem.format
    h[:method] = elem.url_method
    h["RET"] = elem.ret_http_code
    h["HEAD"] = elem.ret_head_status
    h[:content_type] = elem.ret_content_type
    body = elem.ret_body
    h[:size] = body.size

    magic = "?"
    [
      [:json,    -> e { JSON.parse(e) }, JSON::ParserError],
      [:xml,     -> e { e.match(/\A\<\?xml/) }, StandardError],
      [:msgpack, -> e { e.match(/[^[:print:]]+/) }, StandardError],
    ].each do |key, parser, error|
      begin
        if parser.call(body)
          magic = key
          break
        end
      rescue error
      end
    end
    h[:body] = magic

    h[:desc] = elem.desc
  end
end
tt rows
